<div class="text-center">
    <h1>Quản Lý Bài Viết</h1>
</div>
<div class="row" style="
    margin-left: 0;
    margin-right: 0;
">
    <div class="col-1"></div>
    <div class="col-9">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Số Thứ Tự</th>
                    <th scope="col">Tiêu Đề</th>
                    <th scope="col">Xử Lý</th>
                </tr>
            </thead>
            <tbody>
                {{#each DSBV}}
                <tr>
                    <td> {{NewsID}} </td>
                    <td> {{NewsTitle}} </td>
                    <td>
                        <a name="" id="" class="btn btn-outline-secondary" href="/admin/chitiet/?id={{NewsID}}"
                            role="button">Xem Chi Tiết</a>

                    </td>
                </tr>

                {{/each}}

            </tbody>
        </table>
    </div>
    <div class="col-1"></div>
</div>