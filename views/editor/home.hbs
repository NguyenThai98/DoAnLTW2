<div class="container-fluid bientapvien">
    <div class="row mt-4 mb-4">
        <div class="col-sm-12">
            <table class="table text-center">
                <thead>
                    <tr>
                        <th>Tiêu Đề</th>
                        <th>Tình Trạng</th>
                        <th>Xem Chi Tiết</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each list}}
                    <tr>
                        <td> {{NewsTitle}} </td>
                        <td> {{Status}} </td>

                        <td><a class="btn btn-danger" href="/editor/details/{{NewsID}}" role="button">Xem Chi Tiết</a>
                        </td>
                    </tr>

                    {{/each}}
                </tbody>
            </table>
        </div>

    </div>
</div>