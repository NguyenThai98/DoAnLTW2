<!-- bviet noi bat -->
<section style="padding-top: 50px; margin-top:10px;">
    <div class="background">
        <div class="container background-post">
            <div class="title-post text-center">Bài viết nổi bật</div>
            <div class="content-post">
                <div class="row">
                    {{#each lcBVnoiBat}}
                    <div class="col-sm-12 col-md-6">
                        <div class="post-item">
                            <div class="item-img">
                                <a href="/post/detailsCategory/{{NewsID}}"><img src="/public/assets/images/{{Avatar}}"
                                        width="100px" height="100px">
                                </a>
                            </div>
                            <div class="content-post">
                                <div class="post-title"><a href="/post/detailsCategory/{{NewsID}}"> {{NewsTitle}} </a>
                                </div>
                                <div class="post-cre-date"> <i class="fa fa-calendar" aria-hidden="true"></i>
                                    {{DatePost}} </div>
                                <div class="post-category"><a href="#"> {{TagName}} </a></div>
                            </div>
                        </div>
                    </div>
                    {{/each}}

                </div>
            </div>
        </div>
    </div>
</section>
