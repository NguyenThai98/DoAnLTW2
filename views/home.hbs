<section>
    <div class="topTenview">
        <div class="container">
            <div class="item-top-view">
                <div class="title-top-view text-center">
                    Top 10 view
                </div>
                <div class="slick-multiline-top-view">
                    {{#each  lcmuoiBVnoibat}}
                    <div class="col-sm-4">
                        <div class="item">
                            <div class="img-top-view">
                                <a href="/post/detailsCategory/{{NewsID}}"><img src="/public/assets/images/{{Avatar}}">
                                </a>
                            </div>
                            <div class="content-item-top-view">
                                <div class="tw-title"><a href="/post/detailsCategory/{{NewsID}}"> {{NewsTitle}} </a>
                                </div>
                                <div class="tw-date"> {{DatePost}}</div>
                                <div class="tw-category"><a href="#">#{{TagName}}</a></div>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</section>
